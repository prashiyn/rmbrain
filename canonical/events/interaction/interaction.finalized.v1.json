{
  "$id": "https://schemas.rmbrain.ai/events/interaction.finalized.v1.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Interaction Finalized Event Payload",
  "type": "object",
  "required": [
    "interaction_id",
    "relationship_id"
  ],
  "properties": {
    "interaction_id": { "type": "string" },
    "relationship_id": { "type": "string" },
    "client_id": {
      "type": "string",
      "description": "Client involved in the interaction (may be primary_client_id or in scope_client_ids)"
    },
    "interaction_type": { "type": "string" },
    "occurred_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}

